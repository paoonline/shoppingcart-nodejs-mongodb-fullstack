<% layout('../layout') %>
    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Payments</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>

        <!-- <label>Search</label> -->
        <!-- <input class="form-control"> -->
        <!-- <a href="/addproduct" class="btn btn-success " role="button"><i class="fa fa-plus" aria-hidden="true"></i></a> -->

        <div>
            <table class="table table-responsive">
                <thead>
                    <tr>
                        <th>Payment_id</th>
                        <th>PricePayment</th>
                        <th>bank</th>
                        <th>Time</th>
                        <th></th>

                    </tr>
                </thead>
                <tbody>

                    <% payment.forEach(function(payment) { %>

                        <tr>
                            <td required name="orderid" id="orderid">
                                <%= payment.orderids %>
                            </td>
                            <td>
                                <%= payment.pricepayment %>
                            </td>
                            <td>
                                <%= payment.bank %>
                            </td>
                            <td>
                                <%= payment.timepayment %>
                            </td>
                            <td>
                                <!--<button type="submit" class="btn btn-success">Update</button></td>-->
                                <a href="/payment/<%= payment.orderob %>/<%= payment.orderids %>" type="submit" class="btn btn-success">Select</a>
                            </td>
                        </tr>

                        <% }); %>
                </tbody>
            </table>
           
        </div>
    </div>
    <!-- /#page-wrapper -->