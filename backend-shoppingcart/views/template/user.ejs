<% layout('../layout') %>
    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">User</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <form action="/users/idu" method="get" id="user-form"></form>
        <!-- <label>Search</label>
        <input class="form-control" id="search1" required name="search1" onchange="displayname()"> -->
        <!-- <input type= text id="namehere" onchange="displayname()"  required name="namehere"> -->
        <br>

        <!-- <a href="/users/idu" class="btn btn-success " role="button"><i class="fa fa-plus" aria-hidden="true"></i></a> -->

        <script>
            function displayname() {
                location.href =
                    "/users/idu?Key=" + document.getElementById("search1").value;

            }
        </script>
        <div>

            <table class="table table-striped  table-hover" id="mydata">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Email</th>
                    </tr>
                </thead>

                <tbody>
                    <% users1.forEach(function(users1) { %>
                        <tr>
                            <td>
                                <%= users1.id %>
                            </td>
                            <td>
                                <%= users1.email %>
                            </td>
                        </tr>

                        <% }); %>

                </tbody>

            </table>
        </div>
        </form>
    </div>
    <!-- /#page-wrapper -->