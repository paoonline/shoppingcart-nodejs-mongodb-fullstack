<% layout('../layout') %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">User</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <form action="/users" method="post" id="user-form">
    <label>Search</label>
    <input class="form-control" id="search1" required name="search1">
        <br>

        <a href="/user/idu" class="btn btn-success " role="button"><i class="fa fa-plus" aria-hidden="true"></i></a>
    <div>
       

        <table class="table table-responsive">
            <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
            </tr>
            </thead>
            <% users1.forEach(function(users1) { %>
            <tbody>
            <tr>
                <td><%= users1.id %></td>
                <td><%= users1.email %></td>
            </tr>

            <% }); %>

            </tbody>

        </table>
    </div>
    </form>
</div>
<!-- /#page-wrapper -->